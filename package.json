{
        "name": "dua-json-editor",
        "version": "1.0.0",
        "description": "Admin panel for editing Islamic prayers JSON data",
        "type": "module",
        "scripts": {
                "dev": "concurrently \"npm run server\" \"npm run client\"",
                "client": "vite",
                "server": "node server/index.js",
                "build": "vite build",
                "preview": "vite preview",
                "deploy": "netlify deploy --prod",
                "migrate-data": "netlify env:import && node netlify/functions/migrate-data.js"
        },
        "dependencies": {
                "@dnd-kit/core": "^6.3.1",
                "@dnd-kit/sortable": "^10.0.0",
                "@dnd-kit/utilities": "^3.2.2",
                "@emotion/react": "^11.11.1",
                "@emotion/styled": "^11.11.0",
                "@mui/icons-material": "^5.14.19",
                "@mui/material": "^5.14.19",
                "@netlify/blobs": "^10.2.1",
                "cors": "^2.8.5",
                "express": "^4.18.2",
                "react": "^18.2.0",
                "react-dom": "^18.2.0"
        },
        "devDependencies": {
                "@vitejs/plugin-react": "^4.2.0",
                "concurrently": "^8.2.2",
                "vite": "^5.0.5"
        }
}